{
  "name": "tap-flattener",
  "version": "1.0.0",
  "description": "Flattens tap output including subtests into a single level hierarchy",
  "main": "index.js",
  "scripts": {
    "lint": "xo --fix index.js bin/flatten.js test/index.test.js",
    "test": "tap test/*.test.js --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:indexzero/tap-flattener.git"
  },
  "keywords": [
    "tap",
    "flatten",
    "testing"
  ],
  "author": "Charlie Robbins <charlie.robbins@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/indexzero/tap-flattener#readme",
  "bugs": {
    "url": "https://github.com/indexzero/tap-flattener/issues"
  },
  "xo": {
    "space": true
  },
  "devDependencies": {
    "tap": "^10.7.2",
    "xo": "^0.18.2"
  },
  "dependencies": {
    "diagnostics": "^1.1.0",
    "enabled": "^1.0.2",
    "js-yaml": "^3.9.1",
    "minipass": "^2.2.1",
    "tap-parser": "^6.0.1"
  }
}
